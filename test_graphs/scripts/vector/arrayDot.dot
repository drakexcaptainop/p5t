digraph G {
    INP [label="1. input u1: Array, u2: Array"]
    SUM [label="2. sum=0"]
    FOR [label="3. for i=0:u.Length"]
    ADD [label="4. sum=u1[i]+u2[i]"]
    RET [label="5. return sum"]

    INP -> SUM -> FOR 
    FOR -> ADD  [label="in range"]
    FOR -> RET [label="not in range"] 
    ADD -> FOR 
}