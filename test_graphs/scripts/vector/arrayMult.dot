digraph G {
    INP [label="input u: Array, t: Number"]
    RES [label="result=ArrayOfZeros( )"]
    FOR [label="for i=0:u.Length"]
    ADD [label="result[i]*=t"]
    RET [label="return result"]
    INP -> RES -> FOR 
    FOR -> RET [label="not in range"]
    FOR -> ADD [label="in range"]
    ADD -> RET 
}